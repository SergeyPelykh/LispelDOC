<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lispel.lispeldoc">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LispelDOC">
        <activity
            android:name=".activity.InsertEntityInBAseActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AddOrderActivity"
            android:exported="false" />
        <activity
            android:name=".activity.TestNewOrderActivity"
            android:exported="false" />
        <activity
            android:name=".activity.WeirdActivity"
            android:exported="false" />
        <activity
            android:name=".activity.NewOrderActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>